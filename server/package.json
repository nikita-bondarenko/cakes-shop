    "express-handlebars": "^6.0.6",
    "handlebars": "^4.7.7",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "nestjs-config": "^1.4.10",
    "nodemailer": "^6.8.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "process": "^0.11.10",
    "react-intersection-observer": "^9.4.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.23.1",
    "sequelize-cli": "^6.5.1",
    "sequelize-typescript": "^2.1.3",
